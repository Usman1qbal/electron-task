      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "electron": "^8.0.2",
    "electron-builder": "^22.3.2",
    "wait-on": "^4.0.0"
  }
